msgid "Band is blank."
msgstr "频段是空白的。"

#: application/views/logbookadvanced/help.php:12
msgid "Callsign is blank."
msgstr "呼号是空白的。"

#: application/views/logbookadvanced/help.php:13
msgid "Time and date is not set."
msgstr "未设置时间和日期。"

