msgstr "Instalacija"

#: install/run.php:12
msgid "Please wait..."
msgstr "Molimo pričekajte..."

#: install/run.php:15
msgid "Copy config.php to application/config/"
msgstr "Kopirajte config.php u application/config/"

#: install/run.php:18
msgid "Copy database.php to application/config/"
